{
    "name": "jns/xhprof-bundle",
    "type": "symfony-bundle",
    "description": "Symfony JnsXhprofBundle",
    "keywords": ["xhprof"],
    "license": "MIT",
    "homepage": "https://github.com/jonaswouters/XhprofBundle",
    "authors": [
        {
            "name": "Jonas Wouters",
            "email": "hello@jonaswouters.be"
        }
    ],
    "require": {
        "php": "^5.3.2|^7.0",
        "symfony/http-kernel": "^2.3|^3.0",
        "symfony/dependency-injection": "^2.3|^3.0",
        "pbweb/xhprof": "^1.0"
    },
    "require-dev": {
        "doctrine/common": "^2.5",
        "symfony/console":"^2.8|^3.0",
        "phpspec/prophecy": "^1.1.2",
        "phpunit/phpunit": "^4.1.3"
    },
    "suggest": {
        "ext-xhprof": "Hierarchical Profiler for PHP",
        "doctrine/orm": "Persist details in a storage via doctrine",
        "perftools/xhgui": "A graphical interface for XHProf data built on MongoDB"
    },
    "autoload": {
        "psr-4": {
            "Jns\\Bundle\\XhprofBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
