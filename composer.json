    "autoload": {
        "psr-0": {
            "Jns\\Bundle\\XhprofBundle": ""
        }
    },
    "target-dir": "Jns/Bundle/XhprofBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
     }
